	Keywords = {computerised monitoring;peripheral interfaces;power aware computing;PowerMon device;PowerMon2 device;USB interface;commodity computer systems;current monitoring;disks;graphical processing units;measurement channels;power monitoring;voltage monitoring;Computer applications;Computer displays;Computerized monitoring;Current measurement;Energy consumption;Performance analysis;Power measurement;Power supplies;Rails;Voltage},
	Pages = {479-484},
	Title = {PowerMon: Fine-grained and integrated power monitoring for commodity computer systems},
