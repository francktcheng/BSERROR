	Keywords = {mainframes;multiprocessing systems;systems software;DARPA;HPC system efficiency;hardware-software interaction;high performance computing systems;petaFLOPS;power consumption;supercomputer;sustainable exascale computing;system software;Computer architecture;Decision support systems;Hardware;Power demand;Programming;Software;Supercomputers},
	Pages = {270-275},
	Title = {Towards sustainable exascale computing},
	Year = {2010},
